{
    "version": 1,
    "schedule": {
        "type": "simple",
        "interval": "1s"
    },
    "max-failures": 10,
    "workflow": {
        "collect": {
            "metrics": {
                "/intel/psutil/load/load1": {},
                "/intel/psutil/load/load5": {},
                "/intel/psutil/load/load15": {},
                "/intel/psutil/vm/available": {},
                "/intel/psutil/vm/free": {},
                "/intel/psutil/vm/used": {}
            },
            "process": null,
            "publish": [
                {
                    "plugin_name": "influx",                            
                    "config": {
                        "host": "INFLUXDB_IP",
                        "port": 8086,
                        "database": "snap",
                        "user": "admin",
                        "password": "admin"
                    }
                }
            ]                                            
        }
    }
}
