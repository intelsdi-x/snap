        "consumes": [
          "application/json",
          "application/x-protobuf"
        ],
        "produces": [
          "application/json",
          "application/x-protobuf"
        ],
        "schemes": [
          "http",
          "https"
        ],
        "summary": "updates the config of a giving plugin. A wrong plugin type or non-numeric plugin version results in error.",
